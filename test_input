Object1
{
  field_one : int12;
  field_two : u4;
}

Object2
{
  field_three : uint18;
  field_four : s14;
  field_five : float64;
}

Object3
{
  field_six : float32;
  field_seven : bool;
}

ObjectWithCollections
{
  size_field : uint16;
  embedded : Object2;
  collection_one : int32[5]; // hard coded size
  collection_two : bool[size_field]; // sized from field
  collection_three : float64[embedded.field_three];
}
